#test s21_determinant_1
matrix_t matrix_1 = s21_create_matrix(5, 5);
rand_fill(&matrix_1, NULL, ONE, 45, 0, INT);
int det = (int)s21_determinant(&matrix_1);
ck_assert_int_eq(det, -285312);
s21_remove_matrix(&matrix_1);

